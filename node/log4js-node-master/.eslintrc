{
  "root": true,
	"extends": "airbnb-base",
	"rules": {
		"comma-dangle": 0,
		"indent": 2,
		"object-shorthand": 0,
		"func-names": 0,
		"max-len": [1, 120, 2],
		"no-use-before-define": 1,
		"no-param-reassign": 0,
    "strict": 0,
    "import/no-extraneous-dependencies": 1,
    "prefer-spread": 0,
    "prefer-rest-params": 0
	},
  "parser-options": {
    "ecmaVersion": 6
  }
}
